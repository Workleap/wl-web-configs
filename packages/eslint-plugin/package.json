{
    "name": "@workleap/eslint-plugin",
    "author": "Workleap",
    "description": "Workleap recommended ESLint rules and configurations.",
    "version": "3.2.2",
    "keywords": [
        "workleap",
        "eslint",
        "eslintconfig",
        "eslintplugin",
        "eslint-config",
        "eslint-plugin"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-web-configs.git",
        "directory": "packages/eslint-plugin"
    },
    "license": "Apache-2.0",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "types": "./dist/index.d.ts",
    "type":"module",
    "files": [
        "docs",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "lint:eslint": "eslint . --max-warnings=0 --cache --cache-location .cache/eslint"
    },
    "peerDependencies": {
        "eslint": "8.57.0",
        "typescript": "*"
    },
    "peerDependenciesMeta": {
        "@typescript-eslint/parser": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
    },
    "devDependencies": {
        "@swc/core": "1.4.13",
        "@swc/helpers": "0.5.9",
        "@swc/jest": "0.2.36",
        "@types/eslint": "8.56.9",
        "@types/eslint__js": "^8.42.3",
        "@types/estree": "1.0.5",
        "@types/jest": "29.5.12",
        "@types/node": "20.12.7",
        "@workleap/swc-configs": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "tsup": "8.0.2"
    },
    "dependencies": {
        "@eslint/js": "^9.0.0",
        "eslint-flat-config-utils": "^0.2.4",
        "eslint-plugin-jest": "^28.3.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-mdx": "^3.1.5",
        "eslint-plugin-package-json": "^0.12.2",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-testing-library": "^6.2.2",
        "globals": "^15.1.0",
        "jsonc-eslint-parser": "^2.4.0",
        "typescript-eslint": "^7.8.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
