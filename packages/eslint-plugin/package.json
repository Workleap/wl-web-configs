{
    "name": "@workleap/eslint-plugin",
    "author": "Workleap",
    "description": "Workleap ESLint rules and configs.",
    "version": "1.8.2",
    "keywords": [
        "workleap",
        "eslint",
        "eslintconfig",
        "eslintplugin",
        "eslint-config",
        "eslint-plugin"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-web-configs.git",
        "directory": "packages/eslint-plugin"
    },
    "license": "Apache-2.0",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "docs",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "tsup"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "*",
        "eslint-plugin-import": "*",
        "eslint-plugin-jest": "*",
        "eslint-plugin-jsx-a11y": "*",
        "eslint-plugin-react": "*",
        "eslint-plugin-react-hooks": "*",
        "eslint-plugin-storybook": "*",
        "eslint-plugin-testing-library": "*",
        "eslint-plugin-mdx": "*"
    },
    "devDependencies": {
        "@swc/core": "1.3.56",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@types/eslint": "8.37.0",
        "@types/estree": "1.0.0",
        "@types/jest": "29.5.1",
        "@types/node": "20.3.1",
        "@typescript-eslint/parser": "5.58.0",
        "@workleap/swc-configs": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "eslint": "8.38.0",
        "jest": "29.5.0",
        "tsup": "6.7.0",
        "ts-node": "10.9.1",
        "typescript": "5.0.4"
    },
    "peerDependencies": {
        "@typescript-eslint/parser": "*",
        "eslint": "*",
        "typescript": "*"
    },
    "peerDependenciesMeta": {
        "@typescript-eslint/parser": {
            "optional": true
        },
        "eslint": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
