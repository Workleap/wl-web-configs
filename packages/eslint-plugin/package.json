{
    "name": "@workleap/eslint-plugin",
    "author": "Workleap",
    "description": "Workleap recommended ESLint rules and configurations.",
    "version": "3.2.3",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-web-configs.git",
        "directory": "packages/eslint-plugin"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "types": "./dist/index.d.ts",
    "files": [
        "docs",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "tsup"
    },
    "peerDependencies": {
        "@typescript-eslint/parser": "*",
        "eslint": "*",
        "typescript": "*"
    },
    "peerDependenciesMeta": {
        "@typescript-eslint/parser": {
            "optional": true
        },
        "eslint": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^7.6.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^28.2.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-mdx": "^3.1.5",
        "eslint-plugin-package-json": "^0.12.2",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-testing-library": "^6.2.1",
        "eslint-plugin-yml": "^1.14.0",
        "jsonc-eslint-parser": "^2.4.0",
        "yaml-eslint-parser": "^1.2.2"
    },
    "devDependencies": {
        "@swc/core": "1.4.13",
        "@swc/helpers": "0.5.9",
        "@swc/jest": "0.2.36",
        "@types/eslint": "8.56.9",
        "@types/estree": "1.0.5",
        "@types/jest": "29.5.12",
        "@types/node": "20.12.7",
        "@workleap/swc-configs": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "ts-node": "10.9.2",
        "tsup": "8.0.2"
    }
}
