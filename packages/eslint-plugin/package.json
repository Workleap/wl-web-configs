        "@types/estree": "1.0.1",
        "@types/jest": "29.5.5",
        "@types/node": "20.6.2",
        "@typescript-eslint/parser": "6.7.0",
        "@workleap/swc-configs": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "eslint": "8.49.0",
        "jest": "29.7.0",
        "ts-node": "10.9.1",
        "tsup": "7.2.0",
        "typescript": "5.2.2"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
