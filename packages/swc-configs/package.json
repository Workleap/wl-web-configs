{
    "name": "@workleap/swc-configs",
    "description": "Workleap recommended SWC configs.",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "keywords": [
        "workleap",
        "swc",
        "config"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.mjs"
        }
    },
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-web-configs.git",
        "directory": "packages/swc-configs"
    },
    "scripts": {
        "build": "tsup"
    },
    "devDependencies": {
        "@swc/core": "1.3.63",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@types/jest": "29.5.2",
        "@workleap/eslint-plugin": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "jest": "29.5.0",
        "ts-node": "10.9.1",
        "tsup": "6.7.0",
        "typescript": "5.0.4"
    },
    "peerDependencies": {
        "@swc/core": "*",
        "@swc/helpers": "*",
        "@swc/jest": "*"
    },
    "peerDependenciesMeta": {
        "@swc/jest": {
            "optional": true
        }
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
