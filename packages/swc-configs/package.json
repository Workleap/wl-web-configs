{
    "name": "@workleap/swc-configs",
    "description": "Workleap recommended SWC configs.",
    "version": "2.2.3",
    "license": "Apache-2.0",
    "keywords": [
        "workleap",
        "swc",
        "config"
    ],
    "exports": {
        ".": {
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        }
    },
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-web-configs.git",
        "directory": "packages/swc-configs"
    },
    "scripts": {
        "build": "tsup",
        "lint:eslint": "eslint . --max-warnings=0 --cache --cache-location .cache/eslint"
    },
    "peerDependencies": {
        "@swc/core": "*",
        "@swc/helpers": "*",
        "@swc/jest": "*",
        "browserslist": "*"
    },
    "peerDependenciesMeta": {
        "@swc/jest": {
            "optional": true
        },
        "browserslist": {
            "optional": true
        }
    },
    "devDependencies": {
        "@swc/core": "1.4.13",
        "@swc/helpers": "0.5.9",
        "@swc/jest": "0.2.36",
        "@types/jest": "29.5.12",
        "@workleap/eslint-plugin": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "tsup": "8.0.2",
        "typescript": "5.4.5"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
