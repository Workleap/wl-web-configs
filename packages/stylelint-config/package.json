{
    "name": "@workleap/stylelint-config",
    "author": "Workleap",
    "description": "Workleap recommended stylelint config.",
    "version": "1.2.0",
    "license": "Apache-2.0",
    "keywords": [
        "workleap",
        "stylelint",
        "stylelint-plugin",
        "config",
        "css"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-web-configs.git",
        "directory": "packages/stylelint-config"
    },
    "scripts": {
        "build": "tsup"
    },
    "dependencies": {
        "stylelint-config-standard": "32.0.0",
        "stylelint-prettier": "2.0.0"
    },
    "devDependencies": {
        "@workleap/eslint-plugin": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "prettier": "2.8.7",
        "stylelint": "15.4.0",
        "tsup": "6.7.0",
        "typescript": "5.0.4"
    },
    "peerDependencies": {
        "prettier": ">=2.0.0",
        "stylelint": ">=15"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
