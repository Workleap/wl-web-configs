    "devDependencies": {
        "@workleap/eslint-plugin": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "eslint": "8.57.0",
        "tsup": "8.0.2"
