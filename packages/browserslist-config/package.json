{
    "name": "@workleap/browserslist-config",
    "author": "Workleap",
    "description": "Workleap recommended Browserslist configuration.",
    "version": "2.0.1",
    "license": "Apache-2.0",
    "keywords": [
        "workleap",
        "browserslist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-web-configs.git",
