{
    "name": "@workleap/postcss-plugin",
    "author": "Workleap",
    "description": "Workleap's default PostCSS config.",
    "version": "1.2.3",
    "license": "Apache-2.0",
    "keywords": [
        "workleap",
        "postcss",
        "postcss-plugin",
        "css"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-web-configs.git",
        "directory": "packages/postcss-plugin"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
