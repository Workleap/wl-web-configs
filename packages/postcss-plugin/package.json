    "dependencies": {
        "postcss-load-config": "4.0.1",
        "postcss-preset-env": "8.5.1"
    },
    "peerDependencies": {
        "postcss": ">=8.4.6"
    },
    "devDependencies": {
        "@types/node": "20.3.2",
        "@workleap/eslint-plugin": "workspace:*",
        "@workleap/tsup-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "postcss": "8.4.21",
        "tsup": "6.7.0",
        "typescript": "5.0.4"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
