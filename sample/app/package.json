{
    "name": "@sample/app",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Application to try the web configs.",
    "private": true,
    "license": "Apache-2.0",
    "type": "module",
    "msw": {
        "workerDirectory": "public"
    },
    "scripts": {
        "dev": "webpack serve --config webpack.dev.js",
        "dev-msw": "cross-env USE_MSW=true webpack serve --config webpack.dev.js",
