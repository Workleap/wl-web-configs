        "serve-build": "pnpm build && pnpm http-server dist -p 8080 -P http://localhost:8080? -c-1"
    },
    "devDependencies": {
        "@svgr/webpack": "8.0.1",
        "@swc/core": "1.3.66",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@testing-library/react": "14.0.0",
        "@types/jest": "29.5.2",
        "@types/node": "20.3.2",
        "@types/react": "18.2.9",
        "@types/react-dom": "18.2.4",
        "@types/webpack": "5.28.1",
        "@workleap/browserslist-config": "workspace:*",
        "@workleap/eslint-plugin": "workspace:*",
        "@workleap/postcss-configs": "workspace:*",
        "@workleap/stylelint-configs": "workspace:*",
        "@workleap/swc-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "@workleap/webpack-configs": "workspace:*",
        "browserslist": "4.21.7",
        "cross-env": "7.0.3",
        "http-server": "14.1.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "msw": "1.2.2",
        "postcss": "8.4.24",
        "ts-jest": "29.1.0",
        "typescript": "5.0.4",
        "webpack": "5.88.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1",
        "postcss-preset-env": "8.5.1"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.12.1",
        "@sample/components": "workspace:*",
        "@sample/utils": "workspace:*"
    }
